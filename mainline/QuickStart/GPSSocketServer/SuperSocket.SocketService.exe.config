<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
        <section name="socketServer" type="SuperSocket.SocketEngine.Configuration.SocketServiceConfig, SuperSocket.SocketEngine"/>
    </configSections>
    <appSettings>
        <add key="ServiceName" value="GPSSocketServer"/>
    </appSettings>
    <socketServer>
        <servers>
            <server name="GPSSocketServer" serviceName="GPSSocketService"
                    ip="Any" port="2012" maxConnectionNumber="10000">
            </server>
        </servers>
        <services>
            <add name="GPSSocketService"
                 type="SuperSocket.QuickStart.GPSSocketServer.GPSServer, SuperSocket.QuickStart.GPSSocketServer" />
        </services>
    </socketServer>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0" />
    </startup>
</configuration>